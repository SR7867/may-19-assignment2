import math
n=int(input())
seive=[True]*n
seive[0]=seive[1]=False
x=int(math.sqrt(n))
for i in range(2,x+1):
    if seive[i]:
        for j in range(i*i,n,i):
            seive[j]=False
            p=int(input())
            sum=0
        for i in range(1,p+1):
            if seive[p]:
                sum=sum+1
            print(sum)
